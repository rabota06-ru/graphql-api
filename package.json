{
  "name": "graphql-api",
  "version": "1.0.0",
  "description": "API-cервер для сайта rabota06.ru",
  "main": "index.ts",
  "scripts": {
    "start:dev": "npm run prisma:migrate:dev && npm run prisma:generate && nodemon -r tsconfig-paths/register src/index.ts ",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "start:prod": "npm run prisma:generate && prisma migrate deploy && rm -rf ./build && tsc --build && node ./build/src/index.js",
    "build": "rm -rf ./build && tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabota06-ru/graphql-api.git"
  },
  "author": "Movsar Khalakhoev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rabota06-ru/graphql-api/issues"
  },
  "homepage": "https://github.com/rabota06-ru/graphql-api#readme",
  "devDependencies": {
    "@types/graphql-fields": "^1.3.4",
    "@types/node": "^18.11.7",
    "nodemon": "^2.0.20",
    "prisma": "^4.4.0",
    "typegraphql-prisma": "^0.22.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "apollo-server": "^3.10.3",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.20.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.0",
    "type-graphql": "^1.1.1"
  }
}
